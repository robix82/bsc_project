        $(document).ready(function () {

            $("a").click(function() {
                var href = $(this).attr('href');

                if (MManual.Foundation.Url.IsAbsoluteUrl(href)) {
                    var hrefWithoutProtocol = MManual.Foundation.Url.RemoveProtocol(href);

                    var match = externalUrls.some(function(url) {
                        return url.length > 0 && hrefWithoutProtocol.startsWith(url);
                    });

                    if (match) {
                        externalLinkConfirmation.showConfirmationModal(href);
                    } else {
                        return true;
                    }
                } else {
                    return true;
                }
                return false;
            });
        });
    
