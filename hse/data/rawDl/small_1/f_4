    function msieversion() {

        var ua = window.navigator.userAgent;
        var msie = ua.indexOf("MSIE ");

        if (msie > 0 || !!navigator.userAgent.match(/Trident.*rv\:11\./) || msie == NaN)
            var test = $('li.no-children');
        test.addClass("noSpace");
        //console.log("test");

        return false;
    };
